<Page
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <Canvas>
       <Ellipse Panel.ZIndex="1" Canvas.Left="100" Canvas.Top="100" Height ="100" Width="100" Fill ="Gold" />
       <!--canvas.top yukarıdan asağı canvas.left soldan  -->
       <Ellipse  Panel.ZIndex="2"  Canvas.Left="125" Canvas.Top="125" Height ="70" Width="70" Fill ="Tomato" />
       <!--buradaki canvas konumunu verir-->
       <Ellipse  Panel.ZIndex="3"  Canvas.Left="125" Canvas.Top="130" Height ="20" Width="20" Fill ="Black" />
       <!--Panel.ZINDEX ="Görüntülenmesi içindir önceliklenme büyükten küçüğedir"-->
       <TextBox Canvas.Left="375" Canvas.Top="100" Height="30" Width="200" />
       <Label Canvas.Left="300" Content ="Ad Soyad:" Canvas.Top ="100"/>
       <TextBox Canvas.Left="375" Canvas.Top="150" Height="30" Width="200" />
       <Label Canvas.Left="300" Content ="EMAİL" Canvas.Top ="150"/>
       <Button Canvas.Left ="470" Canvas.Top="195" Height="30" Width="100" Content ="Kaydet" />
  </Canvas>
</Page>